<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<spark version="1">
  <files all="true" path="."/>
  <model tick="1">
    <setup>Equations.Model</setup>
    <agents>
      <agent name="SimpleAgent">Equations.SimpleAgent</agent>
    </agents>
    <variables>
      <variable get="get_dt" name="dt" set="set_dt" type="Double"/>
      <variable get="get_k" name="k" set="set_k" type="Double"/>
    </variables>
  </model>
  <interface>
    <about path="readme.txt"/>
    <windows>
      <window height="175" name="Parameters" visible="true" width="632" x="720" y="0"/>
      <window height="668" main="true" name="Main Window" visible="true" width="617" x="0" y="0"/>
    </windows>
    <mainframe control-state="0" dx="0.0" dy="0.0" location="Main Window" rot-x="20.0" rot-y="30.0" zoom="1.0">
      <spacestyle name="space" selected="true" swapXY="false"/>
      <agentstyle alpha-function-value="0.0" border="true" label="false" name="SimpleAgent" position="0" texture-env="0" transparent="false" visible="true"/>
    </mainframe>
    <data-layers/>
    <parameterframe location="Parameters">
      <parameter default="0.1" max="1.0" min="0.01" name="dt" step="0.01" type="Double" variable="dt" widget="Slider"/>
      <parameter default="1.0" max="100.0" min="0.0" name="k" step="0.0" type="Double" variable="k" widget="Slider"/>
    </parameterframe>
  <control-panel delay="8" freq="3"/>
    <variable-sets/>
  </interface>
</spark>
