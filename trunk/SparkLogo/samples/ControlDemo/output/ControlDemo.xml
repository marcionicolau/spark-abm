<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<spark version="1">
  <files all="true" path="."/>
  <model tick="1">
    <setup>ControlDemo.ControlDemo</setup>
    <variables>
      <variable get="get_collisions" name="collisions" set="set_collisions" type="Double"/>
    </variables>
    <agents>
      <agent name="SimpleAgent">ControlDemo.SimpleAgent</agent>
      <agent name="Agent2">ControlDemo.Agent2</agent>
      <agent name="Agent3">ControlDemo.Agent3</agent>
      <agent name="MyLink">ControlDemo.MyLink</agent>
    </agents>
  </model>
  <interface>
    <about path="readme.txt"/>
    <windows>
      <window height="200" name="collisions" visible="true" width="200" x="0" y="0"/>
      <window height="719" main="true" name="Main Window" visible="true" width="715" x="480" y="64"/>
    </windows>
    <mainframe control-state="0" dx="0.0" dy="0.0" location="Main Window" rot-x="20.0" rot-y="30.0" zoom="1.0">
      <spacestyle auto-size="true" cell-xsize="10" cell-ysize="10" name="space" selected="true" swapXY="false"/>
      <agentstyle alpha-function-value="0.0" border="true" label="false" name="Agent3" position="0" texture-env="0" transparent="true" visible="true"/>
      <agentstyle alpha-function-value="0.0" border="true" label="false" name="SimpleAgent" position="1" texture-env="0" transparent="false" visible="true"/>
      <agentstyle alpha-function-value="0.0" border="true" label="false" name="Agent2" position="2" texture-env="0" transparent="false" visible="true"/>
      <agentstyle alpha-function-value="0.0" border="true" label="false" name="MyLink" position="3" texture-env="0" transparent="false" visible="true"/>
    </mainframe>
    
    <data-layers/>
    <chart interval="0" label="collisions" location="collisions" name="collisions" variable="collisions"/>
  <control-panel delay="8" freq="4"/>
  </interface>
</spark>
